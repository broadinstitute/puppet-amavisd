{
  "name": "puppet-amavisd",
  "version": "2.0.1",
  "author": "Andrew Teixeira <teixeira@broadinstitute.org>",
  "summary": "Amavisd Puppet module",
  "license": "Apache-2.0",
  "source": "https://github.com/broadinstitute/puppet-amavisd",
  "project_page": "https://github.com/broadinstitute/puppet-amavisd",
  "issues_url": "https://github.com/broadinstitute/puppet-amavisd/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04",
        "18.04"
      ]
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/apt","version_requirement":">= 1.8.0 < 7.0.0"},
    {"name":"puppetlabs/concat","version_requirement":">= 2.2.1 < 6.0.0"},
    {"name":"puppetlabs/stdlib","version_requirement":">= 1.0.0 < 6.0.0"},
    {"name":"stahnma/epel","version_requirement":">= 1.0.2 < 2.0.0"}
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=3.0.0 < 6.0.0"
    }
  ]
